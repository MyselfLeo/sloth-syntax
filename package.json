{
    "name": "sloth-syntax",
    "displayName": "Sloth Syntax",
    "description": "Syntax highlighting for Sloth",
    "version": "0.9.2",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "sloth",
            "aliases": ["Sloth", "sloth"],
            "extensions": [".slo"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "sloth",
            "scopeName": "source.sloth",
            "path": "./syntaxes/sloth.tmLanguage.json"
        }]
    }
}